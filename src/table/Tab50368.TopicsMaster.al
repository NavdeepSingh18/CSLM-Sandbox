    trigger OnInsert()
    var
        FirstChar: Code[1];
    begin
        TopicMasterRec.Reset();
        TopicMasterRec.SetRange(code, Rec.code);
        if TopicMasterRec.FindSet() then begin
            repeat
                clear(FirstChar);
                FirstChar := CopyStr(TopicMasterRec."Student Last Name Char.", 1, 1);
                if (FirstChar = Rec."first Chara") then
                    error(FirstCharError);
            until TopicMasterRec.Next() = 0;
        end;
        //sdb
        StudentCharacterValidations();
    end;

    trigger OnModify()
    var
        FirstChar: Code[1];
    begin
        TopicMasterRec.Reset();
        TopicMasterRec.SetRange(code, Rec.code);
        if TopicMasterRec.FindSet() then begin
            repeat
                if TopicMasterRec."Entry No." <> Rec."Entry No." then begin
                    clear(FirstChar);
                    FirstChar := CopyStr(TopicMasterRec."Student Last Name Char.", 1, 1);
                    if (FirstChar = Rec."first Chara") then
                        error(FirstCharError);
                end;
            until TopicMasterRec.Next() = 0;
        end;
        StudentCharacterValidations();
    end;

    var
        FirstCharError: textconst ENU = 'first character can not be same in Student last character name for same Topic Code.';
        TopicMasterRec: Record "Topic Master";
}